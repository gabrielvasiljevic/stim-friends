<div class = "container">
	<h3> Users </h3>
	<table class = 'table'>
		<tr>
			<th>Name</th>
			<th>Email</th>
		
		</tr>

		<% _.each(users, function(user) { %>
		<tr data-id="<%= user.id %>" data-model="user">
			<td> <%= user.name %> </td>
			<td> <%= user.email %> </td>
			<td> <a href="/user/addFriend/<%= user.id %>" class = "btn btn-sm btn-primary"> Add </a></td>

		</tr>
		<% }) %>
	</table>


</div>
